{
    "$schema": "https://schemas.wp.org/wp/6.2/theme.json",
    "version": 2,
    "settings": {
        "appearanceTools": true,
        "color": {
            "palette": [
                {
                    "color": "#0f6ed7",
                    "name": "Primary",
                    "slug": "primary"
                },
                {
                    "color": "#181529",
                    "name": "Secondary",
                    "slug": "secondary"
                },
                {
                    "color": "#161c2e",
                    "name": "Tertiary",
                    "slug": "tertiary"
                },
                {
                    "color": "#f9f9f9",
                    "name": "Background",
                    "slug": "background"
                }
            ]
        },
        "layout": {
            "contentSize": "1200px",
            "wideSize": "100%"
        },
        "spacing": {
            "spacingScale": {
                "steps": 0
            },
            "units": [
                "%",
                "px",
                "em",
                "rem",
                "vh",
                "vw"
            ]
        },
        "typography": {
            "fluid": true,
            "fontFamilies": [
                {
                    "fontFamily": "-apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen-Sans, Ubuntu, Cantarell, 'Helvetica Neue', sans-serif",
                    "name": "System Font",
                    "slug": "system-font"
                },
                {
                    "fontFace": [
                        {
                            "fontFamily": "Poppins",
                            "fontStretch": "normal",
                            "fontStyle": "normal",
                            "fontWeight": "100",
                            "src": [
                                "file:../assets/fonts/poppins/Poppins-Thin.ttf"
                            ]
                        },
                        {
                            "fontFamily": "Poppins",
                            "fontStretch": "normal",
                            "fontStyle": "normal",
                            "fontWeight": "200",
                            "src": [
                                "file:../assets/fonts/poppins/Poppins-ExtraLight.ttf"
                            ]
                        },
                        {
                            "fontFamily": "Poppins",
                            "fontStretch": "normal",
                            "fontStyle": "normal",
                            "fontWeight": "300",
                            "src": [
                                "file:../assets/fonts/poppins/Poppins-Light.ttf"
                            ]
                        },
                        {
                            "fontFamily": "Poppins",
                            "fontStretch": "normal",
                            "fontStyle": "normal",
                            "fontWeight": "400",
                            "src": [
                                "file:../assets/fonts/poppins/Poppins-Regular.ttf"
                            ]
                        },
                        {
                            "fontFamily": "Poppins",
                            "fontStretch": "normal",
                            "fontStyle": "normal",
                            "fontWeight": "500",
                            "src": [
                                "file:../assets/fonts/poppins/Poppins-Medium.ttf"
                            ]
                        },
                        {
                            "fontFamily": "Poppins",
                            "fontStretch": "normal",
                            "fontStyle": "normal",
                            "fontWeight": "600",
                            "src": [
                                "file:../assets/fonts/poppins/Poppins-SemiBold.ttf"
                            ]
                        },
                        {
                            "fontFamily": "Poppins",
                            "fontStretch": "normal",
                            "fontStyle": "normal",
                            "fontWeight": "700",
                            "src": [
                                "file:../assets/fonts/poppins/Poppins-Bold.ttf"
                            ]
                        },
                        {
                            "fontFamily": "Poppins",
                            "fontStretch": "normal",
                            "fontStyle": "normal",
                            "fontWeight": "800",
                            "src": [
                                "file:../assets/fonts/poppins/Poppins-ExtraBold.ttf"
                            ]
                        },
                        {
                            "fontFamily": "Poppins",
                            "fontStretch": "normal",
                            "fontStyle": "normal",
                            "fontWeight": "900",
                            "src": [
                                "file:../assets/fonts/poppins/Poppins-Black.ttf"
                            ]
                        }
                    ],
                    "fontFamily": "\"Poppins\", sans-serif",
                    "name": "Poppins",
                    "slug": "Poppins"
                }
            ]
        },
        "useRootPaddingAwareAlignments": true
    },
    "styles": {
        "blocks": {
            "core/site-title": {
                "elements": {
                    "link": {
                        "typography": {
                            "textDecoration": "none"
                        }
                    }
                }
            },
            "core/post-title": {
                "elements": {
                    "link": {
                        "typography": {
                            "textDecoration": "none"
                        }
                    }
                }
            },
            "core/query-pagination-numbers": {
                "elements": {
                    "link": {
                        "typography": {
                            "textDecoration": "none"
                        },
                        "spacing": {
                            "margin": {
                                "left": "10px",
                                "right": "10px"
                            }
                        }
                    }
                }
            },
            "core/query-pagination-previous": {
                "typography": {
                    "textDecoration": "none"
                },
                "elements": {
                    "link": {
                        "typography": {
                            "textDecoration": "none"
                        }
                    }
                }
            },
            "core/query-pagination-next": {
                "typography": {
                    "textDecoration": "none"
                },
                "elements": {
                    "link": {
                        "typography": {
                            "textDecoration": "none"
                        }
                    }
                }
            }
        }
    },
    "templateParts": [
        {
            "area": "header",
            "name": "header",
            "title": "Header"
        },
        {
            "area": "footer",
            "name": "footer",
            "title": "Footer"
        }
    ]
}